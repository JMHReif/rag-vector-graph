{
  "mcpServers": {
    "sec-neo4j": {
      "command": "uvx",
      "args": [ "mcp-neo4j-cypher@0.3.1", "--transport", "stdio"  ],
      "env": {
        "NEO4J_URI": "<your database uri here>",
        "NEO4J_USERNAME": "<your database username here>",
        "NEO4J_PASSWORD": "<your database password here>",
        "NEO4J_DATABASE": "<your database name/instance here>"
      }
    }
  }
}